<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <template id="report_purchaseorder_document" inherit_id="purchase.report_purchaseorder_document">
            <th name="th_description" position="after">
                <th name="th_barcode"><strong>Barcode</strong></th>
            </th>
            <td id="product" position="after">
                <td id="p_barcode">
                    <span t-field="line.product_barcode"/>
                </td>
            </td>
        </template>

        <template id="report_purchasequotation_document" inherit_id="purchase.report_purchasequotation_document">
            <th name="th_description" position="after">
                <th name="th_barcode"><strong>Barcode</strong></th>
            </th>
            <td id="product" position="after">
                <td id="p_barcode">
                    <span t-field="order_line.product_barcode"/>
                </td>
            </td>
        </template>

    </data>
</odoo>